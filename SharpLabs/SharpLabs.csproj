<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>SharpLabs</AssemblyName>
        <RootNamespace></RootNamespace>
        <Configurations>Day 9 Task 1;Day 9 Task 2;Day 10</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>
    
    <PropertyGroup>
        <Configuration>Day 9 Task 1</Configuration>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Day 9 Task 1' ">
      <DebugType>none</DebugType>
      <DefineConstants>DAY_9_TASK_1</DefineConstants>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Day 9 Task 2' ">
        <DebugType>none</DebugType>
        <DefineConstants>DAY_9_TASK_2</DefineConstants>
    </PropertyGroup>


    <PropertyGroup Condition=" '$(Configuration)' == 'Day 10' ">
      <DebugType>none</DebugType>
      <DefineConstants>DAY_10</DefineConstants>
    </PropertyGroup>

</Project>
